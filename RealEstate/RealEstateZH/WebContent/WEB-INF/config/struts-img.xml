<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="imgPackage" namespace="/img" extends="basePackage">
    	<action name="*" class="imgAction" method="{1}">
    		<result name="error">/error.jsp</result>
    		<result name="imgAdd">/WEB-INF/agency/ImgAdd.jsp</result>
    		<result name="imgList">/WEB-INF/agency/ImgList.jsp</result>
    		<result name="success"></result>
    	</action>
    </package>

</struts>