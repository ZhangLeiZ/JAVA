<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

	<bean id="typeDAO" class="com.th.daoImpl.TypeDAOImpl" autowire="byName"></bean>
	
	<bean id="typeService" class="com.th.serviceImpl.TypeServiceImpl" autowire="byName"></bean>
		
	<bean id="typeAction" class="com.th.controller.TypeAction" autowire="byName" scope="prototype"></bean>

</beans>   