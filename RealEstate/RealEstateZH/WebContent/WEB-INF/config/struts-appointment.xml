<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="appointmentPackage" namespace="/appointment" extends="basePackage">
    	<action name="*" class="appointmentAction" method="{1}">
    		<result name="list">/WEB-INF/user/appointmentDetail.jsp</result>
    		<result name="listPage" type="json"></result>
    		<result name="add">/house/single-property.jsp</result>
    		<result name="delete" type="json"></result>
    		
    		<result name="error">/error.jsp</result>
    		<result name="success" type="json"></result>
    		<result name="list1" type="json"></result>
    		<result name="applist">/WEB-INF/agency/AppointmentList.jsp</result>
    		<result name="appfind">/WEB-INF/agency/AppointmentFindList.jsp</result>
    		<result name="null">/WEB-INF/agency/AppointmentNull.jsp</result>
    		<result name="page">/WEB-INF/employee/appointment.jsp</result>
    		<result name="success" type="json"></result>
    	</action>
    </package>

</struts>