<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="agencyPackage" namespace="/agency" extends="basePackage">
    	<action name="*" class="agencyAction" method="{1}">
    		<result name="update">/WEB-INF/agency/agencyUpdate.jsp</result>
    		<result name="del">/WEB-INF/agency/index.jsp</result>
    		<result name="pwd">/WEB-INF/agency/modifypwd.jsp</result>
    		<result name="add">/WEB-INF/agency/index.jsp</result>
    		<result name="login">/WEB-INF/agency/index.jsp</result>
    		<result name="error">/WEB-INF/agency/login.jsp</result>
    	</action>
    </package>

</struts>