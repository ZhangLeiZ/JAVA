<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
    <package name="commentPackage" namespace="/comment" extends="basePackage">
    	<action name="*" class="commentAction" method="{1}">
    		<result name="list">/WEB-INF/user/commentDetail.jsp</result>
    		<result name="listPage" type="json"></result>
    		<result name="status" type="json"></result>
    		<result name="delete" type="json"></result>
    		<result name="commentList" type="json"></result>

    		<result name="error">/error.jsp</result>
    		<result name="list1" type="json"></result>
    		<result name="comList">/WEB-INF/agency/CommentList.jsp</result>
    	</action>
    </package>

</struts>