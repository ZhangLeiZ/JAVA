<?xml version="1.0"?>    
<!DOCTYPE hibernate-mapping PUBLIC     
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"    
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">       
<hibernate-mapping>      
    <class name="com.th.bean.Rolepermission" table="t_role_permission">
			<id name="id" column="id" type="string">
				<generator class="uuid.hex"></generator>
			</id>
			<property name="role_id" length="500" type="string"></property>
			<property name="permission_id" length="50"  type="string"></property>
			<property name="created_time"  type="string"></property>
		</class>
</hibernate-mapping>